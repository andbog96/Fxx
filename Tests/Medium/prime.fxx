(func isPrime (N)
    (prog ()
        (func helper (N divisor)
            (cond (greater (times divisor divisor) N)
                (return true)
                (cond (equal (mod N divisor) 0)
                    (return false)
                    (return (helper N (plus divisor 1)))
                )
            )
        )
    )
)

(isPrime 2)
(isPrime 5)
(isPrime 10)
(isPrime 37)
